{
  "name": "cloudflare-worker-router-typescript",
  "version": "0.0.1",
  "description": "Cloudflare Worker Router Typescript is a lightweight Express-like router library for Cloudflare Workers including service workers, modules, and Durable Objects",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "src/**/*",
    "tsconfig.json",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "buildTests": "node ./scripts/buildTests.js",
    "test": "node --loader ts-node/esm ./scripts/runTests.ts",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connormckelvey/cloudflare-worker-router-typescript.git"
  },
  "keywords": [
    "cloudflare",
    "cloudflare-worker",
    "cloudflare-workers",
    "express",
    "expressjs",
    "framework",
    "middleware",
    "router",
    "routing",
    "worker",
    "workers"
  ],
  "devDependencies": {
    "@cloudflare/workers-types": "^3.3.1",
    "@types/chai": "^4.3.0",
    "@types/glob": "^7.2.0",
    "@types/node": "^17.0.21",
    "chai": "^4.3.6",
    "cloudflare-wrangler-dev": "^1.0.3",
    "esbuild": "^0.14.25",
    "glob": "^7.2.0",
    "start-testing-cloudflare": "^1.0.0",
    "ts-node": "^10.6.0",
    "typescript": "^4.5.5"
  },
  "author": "Connor McKelvey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/connormckelvey/cloudflare-worker-router-typescript/issues"
  },
  "homepage": "https://github.com/connormckelvey/cloudflare-worker-router-typescript#readme"
}
